{
    "manifest_version": 3,
    "name": "Grade Simulator",
    "version": "1.0",
    "description": "Simulate your grades and calculate CGPA and SGPA.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  